<h1>Followers for <%= @user.name %></h1>


<table class="table table-hover">
  <tbody>
    <tr>
      <th>ID</th>
      <th>name</th>
      <th>e-mail</th>
    </tr>
    <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
        </tr>
    <% end %>
  </tbody>
</table>
        